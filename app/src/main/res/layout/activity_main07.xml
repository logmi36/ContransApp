<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:fitsSystemWindows="true"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity07">

<!--    <include-->
<!--        layout="@layout/app_bar"-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="match_parent" />-->

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/app_bar_layout"
        android:fitsSystemWindows="true"
        android:layout_height="wrap_content"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        android:layout_width="match_parent">

        <com.google.android.material.appbar.CollapsingToolbarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:toolbarId="@+id/toolbar"
            app:layout_scrollFlags="scroll|exitUntilCollapsed|enterAlwaysCollapsed"
            app:layout_scrollInterpolator="@android:anim/decelerate_interpolator"
            app:contentScrim="?attr/colorPrimary">

            <ImageView
                android:id="@+id/f07f08"
                android:layout_width="match_parent"
                android:layout_height="400dp"
                android:keepScreenOn="true"
                app:layout_collapseMode="parallax"
                android:src="@drawable/m2"
                android:scaleType="fitXY" />

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/f07t01"
                android:layout_height="?attr/actionBarSize"
                app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
                android:layout_width="match_parent"
                app:title="Contrans"
                app:layout_collapseMode="pin">
            </androidx.appcompat.widget.Toolbar>



        </com.google.android.material.appbar.CollapsingToolbarLayout>
    </com.google.android.material.appbar.AppBarLayout>

    <androidx.core.widget.NestedScrollView
        android:clipToPadding="false"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="Contenidos"
        app:layout_behavior="com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:paddingTop="5dp">


            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:cardElevation="5dp"
                app:cardCornerRadius="10dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginBottom="5dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_marginTop="30dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:textStyle="bold"
                        android:drawableLeft="@drawable/ic_man"
                        android:text=" Nombres"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:id="@+id/f07f01"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="32dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:drawableRight="@drawable/ic_edit_2"
                        android:text=" Nombres"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:drawableLeft="@drawable/ic_man"
                        android:text=" Apellidos"
                        android:textStyle="bold"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />


                    <TextView
                        android:id="@+id/f07f02"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="32dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:drawableRight="@drawable/ic_edit_2"
                        android:text=" apellidos"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />


                    <!--=====================================================================================================-->

                    <TextView
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:drawableLeft="@drawable/ic_mail"
                        android:text=" Correo"
                        android:textStyle="bold"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />


                    <TextView
                        android:id="@+id/f07f04"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="32dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:text=" correo"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />


                    <!--=====================================================================================================-->

                    <TextView
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:text=" Documento de identidad"
                        android:textStyle="bold"
                        android:drawableLeft="@drawable/ic_card"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />


                    <TextView
                        android:id="@+id/f07f05"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="32dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:drawableRight="@drawable/ic_edit_2"
                        android:text=" dni"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <!--=====================================================================================================-->

                    <TextView
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:text=" Celular"
                        android:textStyle="bold"
                        android:layout_gravity="left"
                        android:drawableLeft="@drawable/ic_phone"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />


                    <TextView
                        android:id="@+id/f07f06"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="32dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:drawableRight="@drawable/ic_edit_2"
                        android:text=" celular"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />



                    <!--=====================================================================================================-->

                    <TextView
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="10dp"
                        android:text=" Contraseña"
                        android:drawableLeft="@drawable/ic_key"
                        android:textStyle="bold"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />


                    <TextView
                        android:id="@+id/f07f07"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="32dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="20dp"
                        android:drawableRight="@drawable/ic_edit_2"
                        android:text=" clave"
                        android:layout_gravity="left"
                        android:textSize="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />


                </LinearLayout>

            </androidx.cardview.widget.CardView>

        </LinearLayout>

    </androidx.core.widget.NestedScrollView>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/f07f09"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_anchorGravity="bottom|right|end"
        android:layout_margin="16dp"
        app:layout_anchor="@id/app_bar_layout"
        android:contentDescription="Contenido"
        android:src="@android:drawable/ic_input_add" />"

</androidx.coordinatorlayout.widget.CoordinatorLayout>